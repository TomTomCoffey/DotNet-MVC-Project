@model MyProject.Models.Customer

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<p>@Model.Name </p>

<ul>
    <li>
        Memebership Type: @Model.MembershipType.Name
    </li>

    @if (Model.Birthdate != null)
    {
        <li>
            <p>Birthdate: @Model.Birthdate.Value.ToShortDateString()</p>
        </li>
    }




</ul>