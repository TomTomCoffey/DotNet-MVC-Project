@model MyProject.Models.Movie

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>
        <strong>Genre:</strong>
        @Model.Genre
    </li>
    <li>
       <strong> Release Date: </strong>
        @Model.ReleaseDate
    </li>
    <li>
        <strong>Date Added: </strong>
        @Model.DateAdded
    </li>
    <li>
        <strong>Number in Stock: </strong>
        @Model.NumberInStock
</ul>